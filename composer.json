{
    "name": "mxent/pwax",
    "description": "Progressive Web App built with Laravel & Vue",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "vue",
        "pwa",
        "progressive-web-app",
        "spa",
        "vue-router",
        "pinia"
    ],
    "homepage": "https://github.com/mxent/pwax",
    "authors": [
        {
            "name": "Mxent Open Source Team",
            "email": "opensource@mxent.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "laravel/framework": ">=11.44.1",
        "matthiasmullie/minify": ">=1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Mxent\\Pwax\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Mxent\\Pwax\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mxent\\Pwax\\PwaxServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
