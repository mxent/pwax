{
    "name": "mxent/pwax",
    "description": "Progressive Web App built with Laravel & Vue",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mxent Open Source Team",
            "email": "opensource@mxent.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "illuminate/support": ">=12.0",
        "matthiasmullie/minify": ">=1.3"
    },
    "autoload": {
        "psr-4": {
            "Mxent\\Pwax\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mxent\\Pwax\\PwaxServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
